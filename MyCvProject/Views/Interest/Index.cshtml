@model List<MyCvProject.Models.Entity.TblInterest>    
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h2>Interest List</h2>
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Interest</th>
        <th>Description</th>
    

    </tr>
    @foreach (var item in Model)
    {
        <tr>

            <td>@item.ID</td>
            <td>@item.Description1</td>
            <td>@item.Description2</td>
      
            <td>
                <a href="/Interest/DeleteInterest/@item.ID" class="btn btn-danger btn-circle">
                    <i class="fas fa-trash"></i>
                </a>
            </td>
            <td>
                <a href="/Interest/GetInterest/@item.ID" class="btn btn-info btn-circle">
                    <i class="fas fa-info-circle"></i>
                </a>
            </td>

        </tr>
    }
</table>
<a href="/Interest/AddInterest" class="btn btn-primary">Add New Interest</a>



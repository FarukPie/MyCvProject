@model MyCvProject.Models.Entity.TblEducation
@{
    ViewBag.Title = "AddEducation";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



<h2>Add New Education</h2>
<br />
<form method="post">

    @Html.Label("Title")
    @Html.TextBoxFor(x => x.Title, new { @class = "form-control" })
    @Html.ValidationMessageFor(x => x.Title, "", new { @style = "color :red" })
    <br />
    @Html.Label("Subtitle")
    @Html.TextBoxFor(x => x.SubTitle, new { @class = "form-control" })
    @Html.ValidationMessageFor(x => x.SubTitle, "", new { @style = "color :red" })
    <br />
    @Html.Label("Degree")
    @Html.TextBoxFor(x => x.SubTitle2, new { @class = "form-control" })
    @Html.ValidationMessageFor(x => x.SubTitle2, "", new { @style = "color :red" })
    <br />
    @Html.Label("GPA")
    @Html.TextBoxFor(x => x.GPA, new { @class = "form-control" })
    @Html.ValidationMessageFor(x => x.GPA, "", new { @style = "color :red" })
    <br />
    @Html.Label("Date")
    @Html.TextBoxFor(x => x.Date, new { @class = "form-control" })
    @Html.ValidationMessageFor(x => x.Date, "")
    <br />

    <button class="btn btn-primary">Add New Education</button>
</form>



@using MyCvProject.Models.Entity
@model List<TblEducation>   
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<br />
<h2>Education List</h2>
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Title</th>
        <th>Subtitle</th>
        <th>Degree</th>
        <th>Gpa</th>
        <th>Date</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>

            <td>@item.ID</td>
            <td>@item.Title</td>
            <td>@item.SubTitle</td>
            <td>@item.SubTitle2</td>
            <td>@item.GPA</td>
            <td>@item.Date</td>
    
            <td>
                <a href="/Education/DeleteEducation/@item.ID" class="btn btn-danger btn-circle">
                    <i class="fas fa-trash"></i>
                </a>
            </td>
            <td>
                <a href="/Education/GetEducation/@item.ID" class="btn btn-info btn-circle">
                    <i class="fas fa-info-circle"></i>
                </a>
            </td>

        </tr>
    }
</table>
<a href="/Education/AddEducation" class="btn btn-primary">Add New Education</a>
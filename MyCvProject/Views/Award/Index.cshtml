@using MyCvProject.Models.Entity
@model List<TblSertificate>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h2>Sertificate & Award List</h2>
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Sertifica</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>

            <td>@item.ID</td>
            <td>@item.Description</td>
            
          
            <td>
                <a href="/Award/DeleteSertificate/@item.ID" class="btn btn-danger btn-circle">
                    <i class="fas fa-trash"></i>
                </a>
            </td>
            <td>
                <a href="/Award/GetSertificate/@item.ID" class="btn btn-info btn-circle">
                    <i class="fas fa-info-circle"></i>
                </a>
            </td>

        </tr>
    }
</table>
<a href="/Award/AddSertificate" class="btn btn-primary">Add New Sertificate</a>



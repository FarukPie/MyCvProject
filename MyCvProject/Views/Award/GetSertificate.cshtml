@model MyCvProject.Models.Entity.TblSertificate
@{
    ViewBag.Title = "GetSertificate";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<h2>Update Sertificate</h2>
<br />
<form method="post">
    <br />
    @Html.Label("Sertificate")
    @Html.TextBoxFor(x => x.Description, new { @class = "form-control" })
    @Html.ValidationMessageFor(x => x.Description, "", new { @class = "text-danger" })
    <br />
    <button class="btn btn-primary">Update Sertificate</button>
</form>



